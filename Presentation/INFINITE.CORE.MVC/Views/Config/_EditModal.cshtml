<div class="modal fade" id="ConfigEditModal" tabindex="-1" role="dialog" aria-labelledby="ConfigEditModalLabel" data-backdrop="static">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            @await Html.PartialAsync("~/Views/Shared/Components/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel("Edit Config"))
            <form id="ConfigEditForm" name="ConfigEditForm" role="form" class="form-horizontal">
                <input type="hidden" id="Id_edit" name="Id" />
                <div class="modal-body">
                    <div class="form-group row required">
                        <label class="col-md-3 col-form-label" for="Module">Module</label>
                        <div class="col-md-9">
                            <input id="Module_edit" type="text" name="Module" class="form-control" required>
                        </div>
                    </div>
                    <div class="form-group row required">
                        <label class="col-md-3 col-form-label" for="ConfigKey">Config Key</label>
                        <div class="col-md-9">
                            <input id="ConfigKey_edit" type="text" name="ConfigKey" class="form-control" required>
                        </div>
                    </div>
                    <div class="form-group row required">
                        <label class="col-md-3 col-form-label" for="ConfigValue">Config Value</label>
                        <div class="col-md-9">
                            <input id="ConfigValue_edit" type="text" name="ConfigValue" class="form-control" required>
                        </div>
                    </div>
                </div>
                @await Html.PartialAsync("~/Views/Shared/Components/Modals/_ModalFooterWithSaveAndCancel.cshtml")
            </form>
        </div>
    </div>
</div>